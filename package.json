{
  "name": "tgram-chat",
  "version": "1.0.0",
  "description": "",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "commonjs",
  "scripts": {
    "dev:anonymous": "npx webpack serve --config config/webpack.dev.anonymous.ts & tcm src --watch .",
    "dev:dashboard": "npx webpack serve --config config/webpack.dev.dashboard.ts & tcm src --watch .",
    "build:dashboard:dev": "npx webpack --config config/webpack.dev.dashboard.ts",
    "build:anonymous:dev": "npx webpack --config config/webpack.dev.anonymous.ts",
    "build:dashboard:prod": "npx webpack --config config/webpack.prod.dashboard.ts",
    "build:anonymous:prod": "npx webpack --config config/webpack.prod.anonymous.ts",
    "build:dev": "npm-run-all --parallel build:dashboard:dev build:anonymous:dev",
    "build:prod": "npm-run-all --parallel build:dashboard:prod build:anonymous:prod",
    "build:all": "npm-run-all --parallel build:dev build:prod",
    "generate-script-html": "node ./settings/generateScriptAndHtml.js"
  },
  "author": "",
  "license": "ISC",
